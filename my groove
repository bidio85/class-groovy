    agent any
    stages {
        stage('What is your name') {
           
            steps {
                echo 'My name is Abiodun fas'
                
            }
        }
        stage('Check date') {
           
            steps {
                sh 'date'
               
            }
        }
        stage('Check calendar') {
           
            steps {
                sh 'touch abi.sh'
               
            }
        }
         stage('Class') {
           
            steps {
                echo 'i love myself >> bios.txt'
               
            }
        }
         stage('Situation') {
           
            steps {
                echo 'many many were wanle'
               
            }
        }
         stage('honourable cheche for Rep') {
           
            steps {
                echo 'Kosofe rep 2023'
               
            }
        } 
         stage('honourable cheche') {
           
            steps {
                sh 'touch teacher.sh'
               
            }
        } 
    }
}
